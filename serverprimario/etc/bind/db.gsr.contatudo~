$TTL    60000
@               IN      SOA     serverprimario.contatudo.gsr.    root.serverprimario.contatudo.gsr. (
                        2006031201 ; serial
                        28 ; refresh
                        14 ; retry
                        3600000 ; expire
                        0 ; negative cache ttl
                        )
@               	IN      NS      serverprimario.contatudo.gsr.
